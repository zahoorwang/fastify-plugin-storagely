{
  "name": "@zahoor/fastify-storagely",
  "version": "0.0.1",
  "description": "A Fastify plugin integrating Unstorage, providing base storage, prefixed storage, and snapshot/restore utilities with automatic cleanup on server shutdown.",
  "keywords": [
    "fastify",
    "plugin",
    "plugins",
    "storage",
    "unstorage"
  ],
  "homepage": "https://zahoor.wang/fastify/plugin/storagely",
  "bugs": {
    "url": "https://github.com/zahoorwang/fastify-plugin-storagely/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zahoorwang/fastify-plugin-storagely.git"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/zahoorwang"
    }
  ],
  "license": "MIT",
  "author": "Zahoor Wang <zahoor.wang@gmail.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./drivers/*": "./dist/drivers/*.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "npm run code:fmt && npm run code:lint && npm run code:test && npm run code:build",
    "code:build": "node generate-drivers.mjs && rolldown -c",
    "code:fmt": "oxfmt",
    "code:lint": "oxlint",
    "code:lint:fix": "oxlint --fix",
    "code:test": "node generate-drivers.mjs && vitest run --typecheck.tsconfig tsconfig.test.json --coverage",
    "code:test:ui": "npm run code:test && npx -y serve -l 80 coverage/",
    "package:sort": "sort-package-json",
    "package:taze": "taze major -I --peer --force",
    "test": "npm run code:test"
  },
  "dependencies": {
    "fastify-plugin": "^5.1.0",
    "unstorage": "^1.17.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "fastify": "^5.6.2",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "rolldown": "latest",
    "rolldown-plugin-dts": "^0.18.3",
    "sort-package-json": "^3.5.0",
    "taze": "^19.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
